{
	//"editor.fontFamily": "Fira Code Medium",
	"editor.fontFamily": "JetBrains Mono",
	"editor.fontSize": 15,
	"editor.fontWeight": "500",
	"editor.fontLigatures": true,
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.quickSuggestionsDelay": 200,
	"editor.rulers": [160],
	"editor.renderWhitespace": "boundary",
	"editor.renderLineHighlight": "all",
	"editor.renderIndentGuides": true,
	"editor.dragAndDrop": false,

	"editor.smoothScrolling": true,
	"editor.cursorSmoothCaretAnimation": false,
	"editor.cursorBlinking": "smooth",

	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 160,

	"editor.minimap.renderCharacters": true,
	"editor.minimap.showSlider": "mouseover",
	"editor.minimap.maxColumn": 140,
	"editor.minimap.scale": 1,

	"editor.snippetSuggestions": "inline",
	"editor.hover.delay": 200,
	"editor.suggest.localityBonus": true,
	"editor.suggestSelection": "recentlyUsed",
	"editor.codeActionsOnSave": {
		"source.addMissingImports": true,
		"source.organizeImports": false,
		"source.fixAll.eslint": true,
		"source.fixAll": true
	},

	"editor.formatOnPaste": false,
	"editor.formatOnType": false,
	"editor.formatOnSave": false,
	"editor.autoIndent": "full",
	"editor.renameOnType": true,
	"editor.semanticHighlighting.enabled": true,
	"editor.parameterHints.cycle": true,

	"editor.gotoLocation.multipleDeclarations": "goto",
	"editor.gotoLocation.multipleDefinitions": "goto",
	"editor.gotoLocation.multipleImplementations": "goto",
	"editor.gotoLocation.multipleReferences": "goto",
	"editor.gotoLocation.multipleTypeDefinitions": "goto",
	"editor.multiCursorModifier": "alt",

	"diffEditor.maxComputationTime": 0,

	"breadcrumbs.symbolSortOrder": "name",

	"workbench.editor.enablePreview": false,
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.list.multiSelectModifier": "alt",
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.colorTheme": "Noctis",
	"workbench.settings.editor": "json",
	"workbench.settings.useSplitJSON": true,
	"workbench.tree.indent": 16,
	"workbench.tree.renderIndentGuides": "always",
	"workbench.editor.limit.enabled": false,
	"workbench.editor.scrollToSwitchTabs": true,

	"problems.showCurrentInStatus": true,

	"material-icon-theme.showUpdateMessage": false,
	"material-icon-theme.showReloadMessage": false,
	"material-icon-theme.showWelcomeMessage": false,
	"material-icon-theme.hidesExplorerArrows": true,
	"material-icon-theme.activeIconPack": "react_redux",
	"material-icon-theme.opacity": 0.5,

	"bracketPairColorizer.forceIterationColorCycle": true,
	"bracketPairColorizer.showHorizontalScopeLine": false,
	"bracketPairColorizer.consecutivePairColors": ["()", "[]", "{}", ["Gold", "Aqua", "Green"], "Red"],

	"window.title": "VSCode${separator}${dirty}${rootName}${separator}${activeEditorShort}",
	"window.titleBarStyle": "native",
	"window.menuBarVisibility": "toggle",
	"window.openFilesInNewWindow": "on",
	"window.newWindowDimensions": "maximized",
	"window.restoreWindows": "none",
	"window.closeWhenEmpty": true,
	"window.zoomLevel": 0,
	"window.customMenuBarAltFocus": false,
	"explorer.confirmDelete": false,

	"terminal.integrated.shell.linux": "/bin/zsh",
	"terminal.external.linuxExec": "konsole",
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.experimentalUseTitleEvent": true,
	"terminal.integrated.enableBell": true,
	"terminal.integrated.rendererType": "experimentalWebgl",

	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"files.trimFinalNewlines": true,
	"files.maxMemoryForLargeFilesMB": 8192,
	"files.encoding": "utf8",
	"files.autoSave": "onFocusChange",
	"files.defaultLanguage": "${activeEditorLanguage}",
	"files.associations": {
		"*.mustache": "html",
		"*.user": "json",
		".tslintrc": "jsonc",
		"*.json": "jsonc",
		"*.html": "html",
		"*.go2": "go"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/*.pprof": true,
		"**/*.prof": true,
		"**/*.test": true,
		"**/*.db": true,
		"**/*.min.js": true,
		"**/*compiled.js": true,
		"**/*.js": {
			"when": "$(basename).ts"
		},
		"**/*.map": {
			"when": "$(basename).map"
		},
		"**/node_modules": true
	},

	"search.showLineNumbers": true,
	"search.smartCase": true,
	"search.maintainFileSearchCache": true,
	"search.quickOpen.includeSymbols": true,
	"search.quickOpen.history.filterSortOrder": "recency",
	"search.searchOnType": false,

	"debug.inlineValues": true,
	"debug.toolBarLocation": "floating",
	"debug.showSubSessionsInToolBar": true,
	"debug.enableAllHovers": true,
	"debug.node.useV3": true,
	"debug.chrome.useV3": true,
	"debug.extensionHost.useV3": true,
	"debug.javascript.usePreview": true,
	"debug.onTaskErrors": "abort",

	"[css]": {
		"editor.formatOnSave": false
	},
	"[javascript]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[javascriptreact]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},

	"typescript.suggest.completeFunctionCalls": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.referencesCodeLens.enabled": false,
	"typescript.implementationsCodeLens.enabled": false,
	"typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"typescript.disableAutomaticTypeAcquisition": false,
	"typescript.enablePromptUseWorkspaceTsdk": true,

	"javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"javascript.implicitProjectConfig.checkJs": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"javascript.format.semicolons": "insert",

	"npm.packageManager": "yarn",

	"eslint.enable": true,
	"eslint.run": "onSave",
	"eslint.format.enable": true,
	"eslint.packageManager": "yarn",
	// "eslint.nodePath": "/home/oneofone/.config/yarn/global/node_modules/",
	"eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
	"eslint.lintTask.enable": false,
	"eslint.quiet": false,

	"prettier.disableLanguages": ["vue", "markdown"],
	"prettier.jsxBracketSameLine": true,
	"prettier.singleQuote": false,
	"prettier.packageManager": "yarn",
	"prettier.quoteProps": "consistent",
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "es5",
	"prettier.useTabs": true,
	"prettier.arrowParens": "always",
	"prettier.printWidth": 140,

	"html.format.wrapLineLength": 140,
	"html.format.wrapAttributes": "force-expand-multiline",

	"yaml.format.enable": true,
	"yaml.format.printWidth": 140,

	"python.linting.pylintEnabled": false,
	"python.linting.mypyEnabled": true,
	"python.linting.enabled": true,
	"python.formatting.provider": "black",

	"C_Cpp.clang_format_fallbackStyle": "Google",

	"go.buildOnSave": "off",
	"go.vetOnSave": "off",
	"go.lintOnSave": "off",
	"go.useCodeSnippetsOnFunctionSuggest": true,
	"go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
	"go.autocompleteUnimportedPackages": true,
	"go.testFlags": ["-v", "-count=1", "-benchtime=10s"],
	"go.editorContextMenuCommands": {
		"toggleTestFile": true,
		"addTags": true,
		"removeTags": false,
		"testAtCursor": false,
		"testFile": false,
		"testPackage": true,
		"generateTestForFunction": true,
		"generateTestForFile": false,
		"generateTestForPackage": false,
		"addImport": false,
		"testCoverage": false,
		"playground": false,
		"debugTestAtCursor": true
	},
	"go.addTags": {
		"tags": "json",
		"options": "json=omitempty",
		"promptForTags": false,
		"transform": "camelcase"
	},
	"go.coverOnSingleTest": false,
	"go.gotoSymbol.includeGoroot": true,
	"go.useLanguageServer": true,
	"go.useGoProxyToCheckForToolUpdates": false,
	"go.enableCodeLens": {
		"references": false,
		"runtest": true
	},
	"go.toolsEnvVars": {
		"GOFLAGS":"-tags=go2"
	},
	//"go.languageServerFlags": ["-rpc.trace"],
	"gopls": {
		"env": {},
		"staticcheck": true,
		"analyses": {
			"ST1000": false,
			"ST1003": false,
			"SA5001": false,
			"composites": false
		},
		"codelens": {
			"generate": true,
			"upgrade.dependency": true,
			"test": false,
			"gc_details": true
		},

		"usePlaceholders": true,
		"completeUnimported": true,
		"deepCompletion": true,
		"gofumpt": true
	},

	"rust.all_features": true,

	"html.format.endWithNewline": true,
	"html.format.maxPreserveNewLines": 1,
	"html.format.extraLiners": "-",
	"markdownlint.config": {
		"MD010": false,
		"MD013": false,
		"MD033": false,
		"MD040": false
	},
	"emmet.showSuggestionsAsSnippets": true,
	"php.suggest.basic": false,

	"cSpell.allowCompoundWords": true,
	"cSpell.enabled": false,
	"cSpell.numSuggestions": 10,
	"cSpell.userWords": [
		"ALPN",
		"ASTs",
		"CORS",
		"Cashout",
		"Cmps",
		"Elts",
		"Fira",
		"Gruvbox",
		"IDFAs",
		"Influencers",
		"Injectable",
		"JSONP",
		"Protos",
		"TODO",
		"Unimported",
		"antd",
		"apiserv",
		"apiv",
		"autofetch",
		"boltdb",
		"csel",
		"dups",
		"fatih",
		"favicon",
		"fname",
		"fset",
		"gmail",
		"gogetdoc",
		"goimports",
		"golang",
		"gometalinter",
		"iodb",
		"jsbeautify",
		"jsonp",
		"julienschmidt",
		"konsole",
		"nocheckptr",
		"nolint",
		"oneofone",
		"pidx",
		"pprof",
		"prox",
		"rbuf",
		"redir",
		"retargeting",
		"samp",
		"signup",
		"sqlite",
		"struct",
		"structs",
		"tmpl",
		"uint",
		"ulimit",
		"unmarshals",
		"uuid",
		"wakenn",
		"xast",
		"xerrors",
		"xonsh"
	],
	"cSpell.ignoreRegExpList": ["0x[a-f0-9]+", "<?[\\w.\\-+]+@\\w+(\\.\\w+)+>?"],
	"cSpell.ignorePaths": ["**/node_modules/**", "**/vscode-extension/**", "**/.git/**", ".vscode", "typings", ".gitignore"],
	"cSpell.flagWords": ["hte"],
	"cSpell.spellCheckDelayMs": 100,

	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"git.enableCommitSigning": true,
	"git.autoRepositoryDetection": false,
	"git.untrackedChanges": "separate",
	"timeline.showView": true,

	"gitlens.codeLens.enabled": true,
	"gitlens.advanced.messages": {
		"suppressCommitHasNoPreviousCommitWarning": false,
		"suppressCommitNotFoundWarning": false,
		"suppressFileNotUnderSourceControlWarning": false,
		"suppressGitVersionWarning": false,
		"suppressLineUncommittedWarning": false,
		"suppressNoRepositoryWarning": false,
		"suppressResultsExplorerNotice": false,
		"suppressShowKeyBindingsNotice": true,
		"suppressUpdateNotice": false,
		"suppressWelcomeNotice": false
	},
	"gitlens.keymap": "none",
	"gitlens.statusBar.reduceFlicker": true,
	"gitlens.mode.active": "review",
	"gitlens.menus": {
		"editor": false,
		"editorGroup": {
			"blame": true,
			"compare": true,
			"history": false,
			"remote": false
		},
		"editorTab": {
			"compare": false,
			"history": true,
			"remote": true
		},
		"explorer": {
			"compare": true,
			"history": true,
			"remote": true
		}
	},
	"gitlens.mode.statusBar.enabled": false,
	"gitlens.views.repositories.branches.layout": "list",
	"gitlens.views.fileHistory.enabled": true,
	"gitlens.views.fileHistory.location": "scm",
	"gitlens.views.lineHistory.enabled": true,
	"gitlens.views.lineHistory.location": "scm",

	"githubPullRequests.queries": [
		{
			"label": "Waiting For My Review",
			"query": "is:open review-requested:${user}"
		},
		{
			"label": "Assigned To Me",
			"query": "is:open assignee:${user}"
		},
		{
			"label": "Created By Me",
			"query": "is:open author:${user}"
		}
	],
	"githubIssues.issueCompletions.enabled": false,
	"githubIssues.userCompletions.enabled": false,

	"save-runner.enabled": false,
	"save-runner.commands": [
		{
			"enabled": false,
			"include": "\\.[tj]sx?$",
			"exclude": "/node_modules/",
			"pre": "eslint_d --fix ${tmpFile}"
		},
		{
			"enabled": false,
			"include": "\\.go$",
			"useTempFile": false,
			"pre": "goimports"
		}
	],

	"bookmarks.useWorkaroundForFormatters": true,

	"extensions.autoUpdate": false,
	"extensions.showRecommendationsOnlyOnDemand": true,
	"workbench.settings.enableNaturalLanguageSearch": false,
	"workbench.enableExperiments": false,
	"npm.fetchOnlinePackageInfo": false,
	"telemetry.enableTelemetry": false,
	"telemetry.enableCrashReporter": false,
	"gitlens.advanced.telemetry.enabled": false
}
