{
	//"editor.fontFamily": "Fira Code Medium",
	"editor.fontFamily": "JetBrains Mono",
	"editor.fontSize": 15,
	"editor.fontWeight": "500",
	"editor.fontLigatures": true,
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.quickSuggestionsDelay": 200,
	"editor.rulers": [160],
	"editor.renderWhitespace": "boundary",
	"editor.renderLineHighlight": "all",
	"editor.renderIndentGuides": true,
	"editor.dragAndDrop": false,
	"editor.smoothScrolling": true,
	"editor.cursorSmoothCaretAnimation": false,
	"editor.cursorBlinking": "smooth",
	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 160,
	"editor.minimap.renderCharacters": true,
	"editor.minimap.showSlider": "mouseover",
	"editor.minimap.maxColumn": 140,
	"editor.minimap.scale": 1,
	"editor.snippetSuggestions": "inline",
	"editor.hover.delay": 200,
	"editor.suggest.localityBonus": true,
	"editor.suggestSelection": "recentlyUsed",
	"editor.codeActionsOnSave": {
		"source.addMissingImports": true,
		"source.organizeImports": false,
		"source.fixAll.eslint": true,
		"source.fixAll": true
	},
	"editor.formatOnPaste": false,
	"editor.formatOnType": false,
	"editor.formatOnSave": false,
	"editor.formatOnSaveMode": "file",
	"editor.autoIndent": "full",
	"editor.linkedEditing": true,
	"editor.semanticHighlighting.enabled": true,
	"editor.parameterHints.cycle": true,
	"editor.suggest.showStatusBar": true,
	"editor.maxTokenizationLineLength": 1000000000,
	"files.maxMemoryForLargeFilesMB": 104096,
	"editor.gotoLocation.multipleDeclarations": "goto",
	"editor.gotoLocation.multipleDefinitions": "goto",
	"editor.gotoLocation.multipleImplementations": "goto",
	"editor.gotoLocation.multipleReferences": "goto",
	"editor.gotoLocation.multipleTypeDefinitions": "goto",
	"editor.multiCursorModifier": "alt",
	"editor.find.cursorMoveOnType": true,
	"diffEditor.maxComputationTime": 0,
	"search.experimental.searchInOpenEditors": true,
	"breadcrumbs.symbolSortOrder": "name",
	"workbench.editor.enablePreview": false,
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.list.multiSelectModifier": "alt",
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.colorTheme": "Material Theme Darker High Contrast",
	"workbench.iconTheme": "eq-material-theme-icons-darker",
	"workbench.settings.editor": "json",
	"workbench.settings.useSplitJSON": true,
	"workbench.tree.indent": 16,
	"workbench.tree.renderIndentGuides": "always",
	"workbench.editor.limit.enabled": false,
	"workbench.editor.scrollToSwitchTabs": true,
	"workbench.editor.pinnedTabSizing": "compact",
	"problems.showCurrentInStatus": true,
	"window.title": "${dirty}${rootName}${separator}${activeEditorShort}${separator}VSCode",
	"window.titleBarStyle": "custom",
	"window.menuBarVisibility": "toggle",
	"window.openFilesInNewWindow": "on",
	"window.newWindowDimensions": "maximized",
	"window.restoreWindows": "none",
	"window.closeWhenEmpty": true,
	"window.zoomLevel": 0,
	"window.customMenuBarAltFocus": false,
	"explorer.confirmDelete": false,
	"terminal.integrated.shell.linux": "/bin/zsh",
	"terminal.external.linuxExec": "konsole",
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.experimentalUseTitleEvent": true,
	"terminal.integrated.enableBell": true,
	"terminal.integrated.rendererType": "experimentalWebgl",
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"files.trimFinalNewlines": true,
	"files.encoding": "utf8",
	"files.autoSave": "onFocusChange",
	"files.defaultLanguage": "${activeEditorLanguage}",
	"files.associations": {
		"*.mustache": "html",
		"*.user": "jsonc",
		".tslintrc": "jsonc",
		"*.json": "jsonc",
		"*.html": "html",
		"*.go2": "go"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/*.pprof": true,
		"**/*.prof": true,
		"**/*.test": true,
		"**/*.db": true,
		"**/*.min.js": true,
		"**/*compiled.js": true,
		"**/*.js": {
			"when": "$(basename).ts"
		},
		"**/*.map": {
			"when": "$(basename).map"
		},
		"**/node_modules": true
	},
	"search.showLineNumbers": true,
	"search.smartCase": true,
	"search.maintainFileSearchCache": true,
	"search.quickOpen.includeSymbols": true,
	"search.quickOpen.history.filterSortOrder": "recency",
	"search.searchOnType": false,
	"debug.inlineValues": true,
	"debug.onTaskErrors": "abort",
	"debug.javascript.usePreview": true,
	"debug.showSubSessionsInToolBar": true,
	"debug.allowBreakpointsEverywhere": true,
	"[css]": {
		"editor.formatOnSave": false
	},
	"[javascript]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	},
	"[javascriptreact]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	},
	"[typescriptreact]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	},
	"[typescript]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	},
	"[json]": {
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	},
	"typescript.suggest.completeFunctionCalls": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.referencesCodeLens.enabled": false,
	"typescript.implementationsCodeLens.enabled": false,
	"typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": false,
	"typescript.preferences.quoteStyle": "double",
	"typescript.disableAutomaticTypeAcquisition": false,
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": false,
	"javascript.preferences.quoteStyle": "double",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"javascript.format.semicolons": "insert",
	"js/ts.implicitProjectConfig.checkJs": true,
	"npm.packageManager": "yarn",
	"eslint.run": "onSave",
	"eslint.format.enable": true,
	"eslint.packageManager": "yarn",
	// "eslint.nodePath": "/home/oneofone/.config/yarn/global/node_modules/",
	"eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
	"eslint.lintTask.enable": false,
	"eslint.quiet": false,
	"prettier.jsxBracketSameLine": true,
	"prettier.singleQuote": false,
	"prettier.quoteProps": "consistent",
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "es5",
	"prettier.useTabs": true,
	"prettier.arrowParens": "always",
	"prettier.htmlWhitespaceSensitivity": "ignore",
	"prettier.printWidth": 140,
	"html.format.wrapLineLength": 140,
	"html.format.wrapAttributes": "auto",
	"yaml.format.enable": true,
	"yaml.format.printWidth": 140,
	"python.linting.pylintEnabled": false,
	"python.linting.mypyEnabled": true,
	"python.linting.enabled": true,
	"python.formatting.provider": "black",
	"C_Cpp.clang_format_fallbackStyle": "Google",
	"[go]": {
		"editor.insertSpaces": false,
		"editor.formatOnSave": true,
		"editor.formatOnSaveMode": "file",
		"editor.codeActionsOnSave": {
			"source.organizeImports": true,
			"source.fixAll": true
		}
	},
	"go.alternateTools": {
		"dlv-dap": "dlv"
	},
	"go.lintOnSave": "off",
	"go.testFlags": ["-v", "-count=1", "-benchtime=5s", "-timeout=5m"],
	"go.coverMode": "atomic",
	"go.coverShowCounts": true,
	"go.editorContextMenuCommands": {
		"toggleTestFile": true,
		"addTags": true,
		"removeTags": false,
		"testAtCursor": false,
		"testFile": false,
		"testPackage": true,
		"generateTestForFunction": true,
		"generateTestForFile": false,
		"generateTestForPackage": false,
		"addImport": false,
		"testCoverage": false,
		"playground": false,
		"debugTestAtCursor": true
	},
	"go.addTags": {
		"tags": "json",
		"options": "json=omitempty",
		"promptForTags": false,
		"transform": "camelcase"
	},
	"go.coverOnSingleTest": false,
	"go.toolsManagement.checkForUpdates": "off",
	"go.enableCodeLens": {
		"references": false,
		"runtest": false
	},
	"go.toolsEnvVars": {
		"GO111MODULE": "auto",
		"GOFLAGS": "-tags=go2"
	},
	"go.delveConfig": {
		"dlvLoadConfig": {
			"followPointers": true,
			"maxVariableRecurse": 1,
			"maxStringLen": 128,
			"maxArrayValues": 64,
			"maxStructFields": -1
		},
		"apiVersion": 2,
		"showGlobalVariables": false
	},
	"go.languageServerExperimentalFeatures": {
		"diagnostics": true
	},
	//"go.languageServerFlags": ["-rpc.trace"],
	"gopls": {
		"formatting.gofumpt": true,
		"analyses": {
			"ST1000": false,
			"ST1003": false,
			"SA5001": false,
			"nilness": true,
			"unusedwrite": true,
			// too muchnoise
			"fieldalignment": false,
			"shadow": false,
			"composites": false
		},
		"ui.codelenses": {
			"generate": true,
			"regenerate_cgo": true,
			"test": true,
			"vendor": true,
			"tidy": true,
			"upgrade_dependency": true,
			"gc_details": true
		},
		"ui.diagnostic.annotations": {
			"bounds": true,
			"inline": true,
			"escape": false,
			"nil": false
		},
		"ui.diagnostic.staticcheck": true,
		"ui.completion.usePlaceholders": true,
		"ui.navigation.importShortcut": "Definition",
		"ui.completion.matcher": "Fuzzy",
		"ui.navigation.symbolMatcher": "Fuzzy",
		"ui.navigation.symbolStyle": "Dynamic",
		"ui.completion.completionBudget": "500ms",
		"ui.semanticTokens": true,
		"build.directoryFilters": ["-node_modules", "-data"],
		"build.experimentalPackageCacheKey": true,
		"ui.diagnostic.experimentalDiagnosticsDelay": "300ms",
		"ui.completion.experimentalPostfixCompletions": true
	},
	"rust.all_features": true,
	"html.format.endWithNewline": true,
	"html.format.maxPreserveNewLines": 1,
	"html.format.extraLiners": "-",
	"markdownlint.config": {
		"MD010": false,
		"MD013": false,
		"MD033": false,
		"MD040": false
	},
	"emmet.showSuggestionsAsSnippets": true,
	"php.suggest.basic": false,
