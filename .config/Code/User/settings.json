{
	"editor.fontFamily": "Fira Code Medium",
	"editor.fontSize": 14,
	"editor.fontWeight": "500",
	"editor.fontLigatures": true,
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.quickSuggestionsDelay": 200,
	"editor.rulers": [140],
	"editor.renderWhitespace": "boundary",
	"editor.renderLineHighlight": "all",
	"editor.renderIndentGuides": true,
	"editor.dragAndDrop": false,

	"editor.smoothScrolling": true,
	"editor.cursorSmoothCaretAnimation": false,
	"editor.cursorBlinking": "smooth",

	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 140,

	"editor.minimap.renderCharacters": true,
	"editor.minimap.showSlider": "mouseover",
	"editor.minimap.maxColumn": 140,
	"editor.minimap.scale": 2,

	"editor.snippetSuggestions": "inline",
	"editor.hover.delay": 200,
	"editor.suggest.localityBonus": true,
	"editor.suggestSelection": "recentlyUsed",
	"editor.codeActionsOnSave": {
		"source.fixAll": true
	},
	"editor.formatOnPaste": false,
	"editor.formatOnType": false,
	"editor.formatOnSave": true,
	"editor.autoIndent": "full",
	"editor.semanticHighlighting.enabled": true,

	"diffEditor.maxComputationTime": 0,

	"breadcrumbs.symbolSortOrder": "name",

	"workbench.editor.enablePreview": false,
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.list.multiSelectModifier": "alt",
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorTheme": "Activate UMBRA protocol",
	"workbench.settings.editor": "json",
	"workbench.settings.useSplitJSON": true,
	"workbench.tree.indent": 16,
	"workbench.tree.renderIndentGuides": "always",
	"workbench.editor.limit.enabled": true,

	"problems.showCurrentInStatus": true,

	"materialTheme.accent": "Red",
	"material-icon-theme.showUpdateMessage": false,
	"material-icon-theme.showReloadMessage": false,
	"material-icon-theme.showWelcomeMessage": false,
	"material-icon-theme.hidesExplorerArrows": true,
	"material-icon-theme.activeIconPack": "react_redux",
	"material-icon-theme.opacity": 0.5,
	"editor.tokenColorCustomizations": {
		"comments": "#afafaf88"
	},
	"workbench.colorCustomizations": {
		"sideBar.foreground": "#fff"
	},
	"bracketPairColorizer.forceIterationColorCycle": true,
	"bracketPairColorizer.showHorizontalScopeLine": false,
	"bracketPairColorizer.consecutivePairColors": ["()", "[]", "{}", ["Gold", "Aqua", "Green"], "Red"],
	"window.title": "VSCode${separator}${dirty}${rootName}${separator}${activeEditorShort}",
	"window.titleBarStyle": "custom",
	"window.menuBarVisibility": "compact",
	"window.openFilesInNewWindow": "on",
	"window.newWindowDimensions": "maximized",
	"window.restoreWindows": "none",
	"window.closeWhenEmpty": true,
	"window.zoomLevel": 0,
	"window.customMenuBarAltFocus": false,
	"explorer.confirmDelete": false,

	"terminal.integrated.shell.linux": "/bin/zsh",
	"terminal.external.linuxExec": "konsole",
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.experimentalUseTitleEvent": true,
	"terminal.integrated.enableBell": true,
	"terminal.integrated.rendererType": "experimentalWebgl",

	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"files.trimFinalNewlines": true,
	"files.maxMemoryForLargeFilesMB": 8192,
	"files.encoding": "utf8",
	"files.autoSave": "onFocusChange",
	"files.defaultLanguage": "${activeEditorLanguage}",
	"files.associations": {
		"*.mustache": "html",
		"*.user": "json",
		".tslintrc": "jsonc",
		"*.json": "jsonc",
		"*.html": "html"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/*.pprof": true,
		"**/*.prof": true,
		"**/*.test": true,
		"**/*.db": true,
		"**/*.min.js": true,
		"**/*compiled.js": true,
		"**/*.js": {
			"when": "$(basename).ts"
		},
		"**/*.map": {
			"when": "$(basename).map"
		},
		"**/node_modules": true
	},

	"search.showLineNumbers": true,
	"search.smartCase": true,
	"search.enableSearchEditorPreview": false,
	"search.maintainFileSearchCache": true,
	"search.quickOpen.includeSymbols": true,
	"search.searchOnType": false,

	"debug.inlineValues": true,
	"debug.enableAllHovers": true,
	"debug.javascript.usePreview": true,

	"[css]": {
		"editor.formatOnSave": false
	},
	"[javascript]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.formatOnSave": false,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},

	"typescript.suggest.completeFunctionCalls": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.referencesCodeLens.enabled": false,
	"typescript.implementationsCodeLens.enabled": false,
	"typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"typescript.disableAutomaticTypeAcquisition": false,

	"javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"javascript.implicitProjectConfig.checkJs": true,
	"javascript.updateImportsOnFileMove.enabled": "always",

	"npm.enableScriptExplorer": true,
	"npm.packageManager": "yarn",

	"eslint.enable": true,
	"eslint.packageManager": "yarn",
	"eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],

	"eslint.run": "onSave",

	"prettier.disableLanguages": ["vue", "markdown"],
	"prettier.jsxBracketSameLine": true,
	"prettier.quoteProps": "consistent",
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "es5",
	"prettier.useTabs": true,
	"prettier.arrowParens": "always",
	"prettier.printWidth": 120,

	"yaml.format.enable": true,
	"yaml.format.printWidth": 120,

	"go.buildOnSave": "off",
	"go.vetOnSave": "off",
	"go.lintOnSave": "off",
	"go.useCodeSnippetsOnFunctionSuggest": true,
	"go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
	"go.autocompleteUnimportedPackages": true,
	"go.lintTool": "staticcheck",
	"go.editorContextMenuCommands": {
		"toggleTestFile": true,
		"addTags": true,
		"removeTags": true,
		"testAtCursor": false,
		"testFile": false,
		"testPackage": true,
		"generateTestForFunction": true,
		"generateTestForFile": false,
		"generateTestForPackage": true,
		"addImport": false,
		"testCoverage": true,
		"playground": false
	},
	"go.addTags": {
		"tags": "json",
		"options": "json=omitempty",
		"promptForTags": false,
		"transform": "camelcase"
	},
	"go.coverOnSingleTest": true,
	"go.gotoSymbol.includeGoroot": true,
	"go.useLanguageServer": true,
	"go.languageServerExperimentalFeatures": {
		"format": true,
		"diagnostics": true,
		"documentLink": true
	},
	"[go]": {
		"editor.insertSpaces": false,
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			"source.organizeImports": true
		}
	},
	"go.toolsEnvVars": {
		"GOGC": "3000"
	},
	"gopls": {
		"env": {
			"GOGC": "off"
		},
		"experimentalDisabledAnalyses": ["ST1000", "ST1003"],
		"usePlaceholders": true,
		"completeUnimported": true,
		"staticcheck": true,
		"deepCompletion": true,
		"verboseOutput": false
	},

	"rust.all_features": true,

	"html.format.endWithNewline": true,
	"html.format.maxPreserveNewLines": 1,
	"html.format.extraLiners": "-",
	"markdownlint.config": {
		"MD010": false,
		"MD013": false,
		"MD033": false,
		"MD040": false
	},
	"emmet.showSuggestionsAsSnippets": true,
	"php.suggest.basic": false,
	"cSpell.language": "en,en-US",
	"cSpell.allowCompoundWords": true,
	"cSpell.enabled": false,
	"cSpell.numSuggestions": 10,
	"cSpell.userWords": [
		"ALPN",
		"ASTs",
		"CORS",
		"Cashout",
		"Cmps",
		"Elts",
		"Fira",
		"Gruvbox",
		"Influencers",
		"Injectable",
		"JSONP",
		"Protos",
		"TODO",
		"Unimported",
		"apiserv",
		"apiv",
		"autofetch",
		"boltdb",
		"csel",
		"dups",
		"fatih",
		"fname",
		"fset",
		"gmail",
		"gogetdoc",
		"goimports",
		"golang",
		"gometalinter",
		"jsbeautify",
		"jsonp",
		"julienschmidt",
		"konsole",
		"nolint",
		"oneofone",
		"pidx",
		"pprof",
		"prox",
		"rbuf",
		"redir",
		"retargeting",
		"samp",
		"struct",
		"structs",
		"tmpl",
		"uint",
		"ulimit",
		"unmarshals",
		"xast",
		"xonsh"
	],
	"cSpell.ignoreRegExpList": ["0x[a-f0-9]+", "<?[\\w.\\-+]+@\\w+(\\.\\w+)+>?"],
	"cSpell.ignorePaths": [
		"**/node_modules/**",
		"**/vscode-extension/**",
		"**/.git/**",
		".vscode",
		"typings",
		".gitignore"
	],
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"git.enableCommitSigning": true,
	"git.autoRepositoryDetection": false,
	"git.untrackedChanges": "separate",
	"timeline.showView": true,

	"gitlens.codeLens.enabled": true,
	"gitlens.advanced.messages": {
		"suppressCommitHasNoPreviousCommitWarning": false,
		"suppressCommitNotFoundWarning": false,
		"suppressFileNotUnderSourceControlWarning": false,
		"suppressGitVersionWarning": false,
		"suppressLineUncommittedWarning": false,
		"suppressNoRepositoryWarning": false,
		"suppressResultsExplorerNotice": false,
		"suppressShowKeyBindingsNotice": true,
		"suppressUpdateNotice": false,
		"suppressWelcomeNotice": false
	},
	"gitlens.keymap": "none",
	"gitlens.statusBar.reduceFlicker": true,
	"gitlens.mode.active": "review",
	"gitlens.menus": {
		"editor": false,
		"editorGroup": {
			"blame": true,
			"compare": true,
			"history": false,
			"remote": false
		},
		"editorTab": {
			"compare": false,
			"history": true,
			"remote": true
		},
		"explorer": {
			"compare": true,
			"history": true,
			"remote": true
		}
	},
	"gitlens.mode.statusBar.enabled": false,
	"gitlens.views.repositories.branches.layout": "list",
	"gitlens.views.fileHistory.enabled": true,
	"gitlens.views.fileHistory.location": "scm",
	"gitlens.views.lineHistory.enabled": true,
	"gitlens.views.lineHistory.location": "scm",

	"save-runner.enabled": false,
	"save-runner.commands": [
		{
			"enabled": false,
			"include": "\\.[tj]sx?$",
			"exclude": "/node_modules/",
			"pre": "eslint_d --fix ${tmpFile}"
		},
		{
			"enabled": false,
			"include": "\\.go$",
			"useTempFile": false,
			"pre": "goimports"
		}
	],

	"bookmarks.useWorkaroundForFormatters": true,

	"extensions.autoUpdate": true,
	"extensions.showRecommendationsOnlyOnDemand": true,
	"workbench.settings.enableNaturalLanguageSearch": false,
	"workbench.enableExperiments": false,
	"npm.fetchOnlinePackageInfo": false,
	"telemetry.enableTelemetry": false,
	"telemetry.enableCrashReporter": false,
	"gitlens.advanced.telemetry.enabled": false
}
