{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "json",
    "go"
  ],
  "go.checkForUpdates": false,
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings": {
    "<cr>": [
      "expandable?",
      "expand",
      "open"
    ],
    "v": "open:vsplit",
    "h": "open:hsplit"
  }
}
